{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", "group:recommended", "schedule:weekly"],
  "commitMessagePrefix": "build(deps):",
  "labels": ["dependencies"],
  "automerge": true,
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackageNames": ["mjml", "@types/mjml"],
      "groupName": "mjml packages",
      "automerge": false
    }
  ]
}
